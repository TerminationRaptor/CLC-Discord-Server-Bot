{{ $args := parseArgs 2 "" (carg "member" "target member") (carg "string" "award name") }}

{{ $member := $args.Get 0 }}
{{ $id := ($args.Get 0).User.ID }}
{{ $award := $args.Get 1 }}
{{ $awardList := sdict }}
{{ $key := 0 }}

{{ while $awardList.HasKey (toString $key) }}
	{{ $key = add $key 1 }}
    {{ $awardList.HasKey (toString $key) }}
{{ end }}

{{ $awardList.Set (toString $key) $award }}
{{ dbGet $id "awardList" }}
{{ dbSet $id "awardList" $awardList }}
